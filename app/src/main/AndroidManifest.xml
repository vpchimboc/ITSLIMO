<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tecnologico.limon.itslimon">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/i_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/i_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.ITSLIMON">
        <activity android:name=".ContabilidadActivity"></activity>
        <activity android:name=".SoftwareActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.ITSLIMON.NoActionBar" />
        <activity
            android:name=".SplasActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>